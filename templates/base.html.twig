<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>{% block title %}Maretraite Hotel{% endblock %}</title>

		{% block meta %}
		<meta name="viewport" content="width=device-width, initial-scale=1"/>
		{% endblock %}

		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}
	</head>
	<body>
		{% block body %}{% endblock %}
		
		{% if isDebug %}
			{% block debug %}{% endblock %}
			<footer class="debug-info container-fluid">
				<pre>
					{{ dump() }}
				</pre>
			</footer>
		{% endif %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</body>
</html>
